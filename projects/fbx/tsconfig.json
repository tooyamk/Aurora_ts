{
    "compilerOptions": {
        "composite": true,
        "declaration": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "sourceMap": true,
        "module": "amd",
        "target": "es6",
        "removeComments": false,
        "preserveConstEnums": true,
        "listFiles": true,
        "listEmittedFiles": true,
        "outFile": "../../dist/aurora.fbx.js"
    },
    "include": [
        "./**/*.ts", "../examples/zlib.d.ts"
    ],
    "exclude": [],
    "compileOnSave": true,
    "references": [
        {
            "path": "../core"
        }
    ]
}